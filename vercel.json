{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/app.js" }
    ],
    "env": {
      "EXAMPLE_ENV_VAR": "example-value"
    },
    "install": {
      "include": ["package.json"],
      "ignoreScripts": false,
      "dest": "/vercel/functions",
      "cmd": "npm install --force"
    },
    "functions": {
      "app": {
        "runtime": "@vercel/node"
      }
    }
  }
  