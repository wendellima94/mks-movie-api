{
    "version": 2,
    "builds": [
      {
        "src": "main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/main.js" }
    ],
    "functions": {
      "app": {
        "runtime": "@vercel/node"
      }
    }
  }
  
